{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/messagetracing",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
